{
  "name": "spx-data",
  "version": "1.0.0",
  "description": "SPX data pipeline built with Effect-TS",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "bun run src/main.ts",
    "test": "bun test test --ignore=\"**/integration/**\"",
    "test:all": "bun test --timeout 30000",
    "test:unit": "bun test test --ignore=\"**/integration/**\"",
    "test:integration": "THETA_DATA_TERMINAL_URL=http://127.0.0.1:25510 bun test --timeout 30000 integration",
    "test:watch": "bun test test --watch --ignore=\"**/integration/**\"",
    "build": "bun run scripts/build.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "health": "bun run dist/spx-data health"
  },
  "keywords": [
    "effect",
    "typescript",
    "data-pipeline",
    "spx"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@effect/cli": "^0.69.0",
    "@effect/platform": "^0.90.0",
    "@effect/platform-bun": "^0.77.0",
    "@effect/schema": "^0.75.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "effect": "^3.17.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/bun": "1.0.8",
    "typescript": "5.3.3",
    "vitest": "^3.2.4"
  }
}
